{
  "version": 2,
  "builds": [
    { "src": "someday.js", "use": "@now/node-server" }
  ],
  "routes": [
    { "src": "/trips/(.*)", "dest": "someday.js" },
    { "src": "/trips/(.*)/(.*)", "dest": "someday.js" },
    { "src": "/destination/(.*)", "dest": "someday.js" },
    { "src": "/destination/(.*)/sight/(.*)", "dest": "someday.js" }
  ]
}