{
  "name": "someday",
  "version": "0.5.0",
  "description": "for travellers",
  "main": "someday.js",
  "scripts": {
    "prestart": "npm run compile",
    "start": "node someday.js",
    "dev": "node someday.js & npm run compile:watch",
    "compile": "export NODE_ENV=production && webpack --output-path public/",
    "compile:watch": "export NODE_ENV=development && webpack --output-path public/ --watch",
    "lint:fix": "eslint . --ext .jsx --ext .js  --fix",
    "lint": "eslint . --ext .jsx --ext .js",
    "bundlesize": "bundlesize",
    "test": "npm run test:unit && npm run compile && npm run bundlesize",
    "test:unit": "mocha --require babel-core/register tests/**/*"
  },
  "author": "Jdlrobson",
  "license": "ISC",
  "dependencies": {
    "@babel/plugin-transform-react-jsx": "7.2.0",
    "@babel/preset-env": "7.2.3",
    "@babel/register": "7.0.0",
    "babel-core": "6.26.3",
    "babel-loader": "8.0.5",
    "connect-memcached": "0.2.0",
    "css-loader": "2.1.0",
    "domino": "2.0.2",
    "eslint": "5.6.1",
    "eslint-plugin-react": "7.11.1",
    "express": "4.16.3",
    "express-session": "1.15.6",
    "express-wikimedia-api-proxy": "1.1.1",
    "express-wikimedia-collections": "0.9.0",
    "extract-text-webpack-plugin": "3.0.2",
    "fg-loadcss": "2.1.0",
    "hogan-express": "0.5.2",
    "isomorphic-fetch": "2.2.1",
    "leaflet": "1.3.4",
    "less": "3.9.0",
    "less-loader": "4.1.0",
    "md5-hex": "2.0.0",
    "memorystore": "1.6.0",
    "mini-css-extract-plugin": "0.4.4",
    "navigo": "7.1.2",
    "node-cache": "4.2.0",
    "oauth-fetch-json": "0.3.3",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "passport-mediawiki-oauth": "github:jdlrobson/passport-mediawiki-oauth",
    "path-to-regexp": "2.2.1",
    "prettier": "1.12.1",
    "query-string": "6.1.0",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-leaflet": "2.0.1",
    "sw-toolbox": "3.6.0",
    "uglifyjs-webpack-plugin": "2.0.1",
    "url-loader": "^1.1.2",
    "webpack": "4.26.1",
    "webpack-cli": "3.1.2",
    "wikipedia-react-components": "1.8.3"
  },
  "bundlesize": [
    {
      "path": "public/page.css",
      "maxSize": "5.6KB"
    }
  ]
}
