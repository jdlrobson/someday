{
  "name": "someday",
  "version": "0.5.0",
  "description": "for travellers",
  "main": "someday.js",
  "scripts": {
    "prestart": "npm run compile",
    "start": "node someday.js",
    "dev": "node someday.js & npm run compile:watch",
    "compile": "export NODE_ENV=production && webpack --output-path public/",
    "compile:watch": "export NODE_ENV=development && webpack --output-path public/ --watch",
    "lint:fix": "eslint . --ext .jsx --ext .js  --fix",
    "lint": "eslint . --ext .jsx --ext .js",
    "test": "mocha --require babel-core/register tests/**/*"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "env"
    ]
  },
  "author": "Jdlrobson",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "connect-memcached": "^0.2.0",
    "domino": "^2.0.2",
    "eslint-plugin-react": "^7.8.1",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "express-wikimedia-api-proxy": "^1.1.1",
    "express-wikimedia-collections": "^0.9.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "hogan-express": "^0.5.2",
    "isomorphic-fetch": "^2.2.1",
    "leaflet": "^1.3.4",
    "md5-hex": "^2.0.0",
    "memorystore": "^1.6.0",
    "mini-css-extract-plugin": "^0.4.4",
    "navigo": "^7.1.2",
    "node-cache": "^4.2.0",
    "oauth-fetch-json": "^0.3.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "passport-mediawiki-oauth": "github:jdlrobson/passport-mediawiki-oauth",
    "path-to-regexp": "^2.2.1",
    "query-string": "^6.1.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-leaflet": "^2.0.1",
    "sw-toolbox": "^3.6.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "wikipedia-react-components": "^1.8.1"
  },
  "devDependencies": {
    "eslint": "^5.6.1",
    "eslint-plugin-react": "^7.11.1",
    "prettier": "1.12.1",
    "webpack-cli": "^3.1.2"
  }
}
